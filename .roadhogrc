{
  "entry": "src/index.js",
  "disableCSSModules":true,
  "publicPath": "/deepviz/", //build for github page need
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        ["transform-runtime", {"helpers": false,
          "polyfill": false,
          "regenerator": true}],
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    }
  }
}
